[run]
branch = True
source = ./csep/
omit =
    tests/*
    docs/*
    examples/*
    csep/utils/constants.py
    csep/utils/datasets.py
    csep/utils/documents.py
    setup.py


[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:

ignore_errors = True